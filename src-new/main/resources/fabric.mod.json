{
  "schemaVersion": 1,
  "id": "minmod",
  "version": "${version}",
  "name": "MinMod",
  "description": "A minimalist Minecraft mod with planes",
  "authors": [
    "Malek"
  ],
  "contact": {
    "homepage": "https://github.com/malek/minmod",
    "sources": "https://github.com/malek/minmod"
  },
  "license": "MIT",
  "icon": "assets/minmod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "org.malek.minmod.Minmod"
    ],
    "client": [
      "org.malek.minmod.client.MinmodClient"
    ]
  },
  "mixins": [
    "minmod.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "minecraft": "~${minecraft_version}",
    "java": ">=17",
    "fabric-api": "*"
  },
  "suggests": {
    "another-mod": "*"
  }
